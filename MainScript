	local text2 = swing2:FindFirstChild("TextLabel")
	
	local function SWNG1()
		text2.Font = Enum.Font.Montserrat
		text1.Font = Enum.Font.MontserratBold
	end
	
	local function SWNG2()
		text1.Font = Enum.Font.Montserrat
		text2.Font = Enum.Font.MontserratBold
	end
	
	
	
	swing1.MouseButton1Click:Connect(SWNG1)
	
	swing2.MouseButton1Click:Connect(SWNG2)
	
	
end
