-- Scripts:

local function ESECL_fake_script() -- Title.LocalScript 
	local script = Instance.new('LocalScript', Title)

	local textlabel = script.Parent
	
	while true do
		wait(10)
		textlabel.Text = "S41t-Xpl01t"
		wait(0.01)
		textlabel.Text = "Sh1t-Xpl01t"
		wait(0.01)
		textlabel.Text = "Shit-Xpl01t"
		wait(0.01)
		textlabel.Text = "Shit-Xplo1t"
		wait(0.01)
		textlabel.Text = "Shit-Xploit"
		wait(1)
		textlabel.Text = "S41t-Xpl01t"
		wait(0.01)
		textlabel.Text = "Sh1t-Xpl01t"
		wait(0.01)
		textlabel.Text = "Shit-Xpl01t"
		wait(0.01)
		textlabel.Text = "Shit-Xplo1t"
		wait(0.01)
		textlabel.Text = "Shit-Xploit"
	end
end
coroutine.wrap(ESECL_fake_script)()
local function DOGYKZV_fake_script() -- Spaceship.LocalScript 
	local script = Instance.new('LocalScript', Spaceship)

	local button = script.Parent
	local SPCEBUTTON = script.Parent.Parent.Parent.swingSelector.Swing1
	local SPCEBUTTON2 = script.Parent.Parent.Parent.swingSelector.Swing2
	
	local search = script.Parent.Parent.Parent.background.TextBox
	local plr = game.Players.LocalPlayer
	local char = plr.Character
	local targetPlayer = nil
	local selectedSpace = "Space1"
	
	local function findTargetPlayer()
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Name:lower() == search.Text:lower() or player.DisplayName:lower() == search.Text:lower() then
				return player
			end
		end
		return nil
	end
	
	-- Space1 logic
	local function Space1()
		if targetPlayer then
			local char2 = targetPlayer.Character
			local play = game.Workspace.Map.ClubIsland.Playround
	
			local function getPlayer()
				if char2 then
					for _ = 1, 25 do
						play.Equiptment.SwingSet.Swing1.SwingSeat.CFrame = char2.HumanoidRootPart.CFrame + Vector3.new(0, -2, 0)
						wait(0.00001)
					end
					for _ = 1, 100 do
						play.Equiptment.SwingSet.Swing1.SwingSeat.CFrame = CFrame.new(0, 10000000, 0)
						wait(0.00001)
					end
				end
			end
	
			local originalPosition = char.HumanoidRootPart.CFrame
	
			local function teleportBack()
				local humanoidRootPart = char:WaitForChild("HumanoidRootPart")
				humanoidRootPart.CFrame = play.Equiptment.SwingSet.Swing1.SwingSeat.CFrame
	
				wait(0.5)
	
				humanoidRootPart.CFrame = originalPosition
	
			end
	
			local function fixRope()
				play.Equiptment.SwingSet.Swing1.LeftRope.Length = 1000000
				play.Equiptment.SwingSet.Swing1.RightRope.Length = 1000000
			end
	
			teleportBack()
	
			wait(0.2)
	
			fixRope()
	
			wait(0.5)
	
			getPlayer()
		end
	end
	
	local function Space2()
		if targetPlayer then
			local char2 = targetPlayer.Character
			local play = game.Workspace.Map.ClubIsland.Playround
	
			local function getPlayer()
				if char2 then
					for _ = 1, 25 do
						play.Equiptment.SwingSet.Swing2.SwingSeat.CFrame = char2.HumanoidRootPart.CFrame + Vector3.new(0, -2, 0)
						wait(0.00001)
					end
					for _ = 1, 100 do
						play.Equiptment.SwingSet.Swing2.SwingSeat.CFrame = CFrame.new(0, 10000000, 0)
						wait(0.00001)
					end
				end
			end
	
			local originalPosition = char.HumanoidRootPart.CFrame
	
			local function teleportBack()
				local humanoidRootPart = char:WaitForChild("HumanoidRootPart")
				humanoidRootPart.CFrame = play.Equiptment.SwingSet.Swing2.SwingSeat.CFrame
	
				wait(0.5)
	
				humanoidRootPart.CFrame = originalPosition
	
			end
	
			local function fixRope()
				play.Equiptment.SwingSet.Swing2.LeftRope.Length = 1000000
				play.Equiptment.SwingSet.Swing2.RightRope.Length = 1000000
			end
	
			teleportBack()
	
			wait(0.2)
	
			fixRope()
	
			wait(0.5)
	
			getPlayer()
		end
	end
	
	SPCEBUTTON.MouseButton1Click:Connect(function()
		selectedSpace = "Space1"
	end)
	
	SPCEBUTTON2.MouseButton1Click:Connect(function()
		selectedSpace = "Space2"
	end)
	
	button.MouseButton1Click:Connect(function()
		targetPlayer = findTargetPlayer()
	
		if not targetPlayer then
			warn("No target player found!")
			return
		end
	
		if selectedSpace == "Space1" then
			Space1()
		elseif selectedSpace == "Space2" then
			Space2()
		else
			warn("No space button selected!")
		end
	end)
	
	
	
	
end
coroutine.wrap(DOGYKZV_fake_script)()
local function UWLF_fake_script() -- GP.LocalScript 
	local script = Instance.new('LocalScript', GP)

	local button = script.Parent
	local SWNGBUTTON = script.Parent.Parent.Parent.swingSelector.Swing1
	local SWNGBUTTON2 = script.Parent.Parent.Parent.swingSelector.Swing2
	
	local search = script.Parent.Parent.Parent.background.TextBox
	local plr = game.Players.LocalPlayer
	local char = plr.Character
	local targetPlayer = nil
	local selectedSwing = "Swing1" 
	
	local function findTargetPlayer()
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Name:lower() == search.Text:lower() or player.DisplayName:lower() == search.Text:lower() then
				return player
			end
		end
		return nil
	end
	
	local function swing1()
		if targetPlayer then
			local char2 = targetPlayer.Character
			local play = game.Workspace.Map.ClubIsland.Playround
	
	
			play.Equiptment.SwingSet.Swing1.LeftRope.Length = 100000
			play.Equiptment.SwingSet.Swing1.RightRope.Length = 100000
	
			wait(0.1)
	
			char.HumanoidRootPart.CFrame = play.Equiptment.SwingSet.Swing1.SwingSeat.CFrame + Vector3.new(7, 5, 0)
	
			wait(0.1)
	
			play.Equiptment.SwingSet.Swing1.SwingSeat.CFrame = char2.HumanoidRootPart.CFrame
		end
	end
	
	local function swing2()
		if targetPlayer then
			local char2 = targetPlayer.Character
			local play = game.Workspace.Map.ClubIsland.Playround
	
	
			play.Equiptment.SwingSet.Swing2.LeftRope.Length = 100000
			play.Equiptment.SwingSet.Swing2.RightRope.Length = 100000
	
			wait(0.1)
	
			char.HumanoidRootPart.CFrame = play.Equiptment.SwingSet.Swing2.SwingSeat.CFrame + Vector3.new(7, 5, 0)
	
			wait(0.1)
	
			play.Equiptment.SwingSet.Swing2.SwingSeat.CFrame = char2.HumanoidRootPart.CFrame
		end
	end
	
	-- Button click handlers
	SWNGBUTTON.MouseButton1Click:Connect(function()
		selectedSwing = "Swing1"
	end)
	
	SWNGBUTTON2.MouseButton1Click:Connect(function()
		selectedSwing = "Swing2"
	end)
	
	button.MouseButton1Click:Connect(function()
		-- Update targetPlayer based on the current TextBox value
		targetPlayer = findTargetPlayer()
	
		if not targetPlayer then
			warn("No target player found!")
			return
		end
	
		if selectedSwing == "Swing1" then
			swing1()
		elseif selectedSwing == "Swing2" then
			swing2()
		else
			warn("No swing button selected!")
		end
	end)
	
end
coroutine.wrap(UWLF_fake_script)()
local function ARODGMK_fake_script() -- TP.LocalScript 
	local script = Instance.new('LocalScript', TP)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		local search = script.Parent.Parent.Parent.background.TextBox
		local plr = game.Players.LocalPlayer
		local char = plr.Character
		
		
		for _, player in pairs(game.Players:GetPlayers()) do
			if player.Name:lower() == search.Text:lower() or player.DisplayName:lower() == search.text:lower() then
				targetPlayer = player
				break
			end
		end
	
		-- Check if the target player was found
		if targetPlayer then
			local char2 = targetPlayer.Character
			
			char.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
			
		end
	end)
end
