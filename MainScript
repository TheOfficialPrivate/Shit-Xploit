local function XZTW_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	local textBox = script.Parent
	local suggestionLabel = script.Parent.Parent:WaitForChild("SuggestionLabel") -- Adjust the path if necessary
	local players = game:GetService("Players")
	
	-- Function to get all player names
	local function getAllPlayerNames()
		local playerNames = {}
		for _, player in pairs(players:GetPlayers()) do
			table.insert(playerNames, player.Name)
			table.insert(playerNames, player.DisplayName) -- Add display names
		end
		return playerNames
	end
	
	-- Function to find the first matching player name
	local function findFirstMatchingPlayerName(input)
		local playerNames = getAllPlayerNames()
		for _, name in pairs(playerNames) do
			if name:lower():sub(1, #input) == input:lower() then
				return name
			end
		end
		return nil
	end
	
	-- Function to show suggestions while typing
	local function showSuggestions()
		local input = textBox.Text
		local suggestion = findFirstMatchingPlayerName(input)
	
		if suggestion and input ~= "" then
			-- Show the suggestion in the TextLabel
			suggestionLabel.Text = input .. suggestion:sub(#input + 1)
		else
			-- Clear the suggestion if no match is found or input is empty
			suggestionLabel.Text = ""
		end
	end
	
	-- Function to auto-complete player name when Enter is pressed
	local function autoCompletePlayerName()
		local input = textBox.Text
		local suggestion = findFirstMatchingPlayerName(input)
	
		if suggestion and input ~= "" then
			textBox.Text = suggestion
		end
	end
	
	-- Connect to the TextBox events
	textBox:GetPropertyChangedSignal("Text"):Connect(showSuggestions)
	textBox.Focused:Connect(function()
		textBox.Text = ""
		suggestionLabel.Text = ""
	end)
	textBox.FocusLost:Connect(function()
		suggestionLabel.Text = ""
	end)
	
	-- Connect to the UserInputService to detect Enter key press
	game:GetService("UserInputService").InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.Return then
			autoCompletePlayerName()
		end
	end)
end
coroutine.wrap(XZTW_fake_script)()
local function NCNEXIN_fake_script() -- Main.DragScript 
	local script = Instance.new('LocalScript', Main)

	--Not made by me, check out this video: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	--Put this inside of your Frame and configure the speed if you would like.
	--Enjoy! Credits go to: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(NCNEXIN_fake_script)()
local function CVFG_fake_script() -- swingSelector.LocalScript 
